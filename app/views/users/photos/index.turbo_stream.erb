<%= turbo_stream.append "photo-list" do %>
    <%= render partial: "users/photos/shared/photo", locals: {photos: @photos} %>
<% end %>

<% if @next_page.present? %>
    <%= turbo_stream.replace "load_more" do %>
        <%= turbo_frame_tag "load_more", src: users_photos_path(page: @next_page, format: :turbo_stream), loading: :lazy %>
    <% end %>
<% end %>